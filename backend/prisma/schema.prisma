datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  publishedAt DateTime
  status      String  
  tags        Tag[]    @relation("ProjectTags")
  public      Boolean
  link        String
}

model Tag {
  id       Int      @id @default(autoincrement())
  name     String
  projects Project[] @relation("ProjectTags")
}
